{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "structure data required for geometry manipulation",
  "type": "object",
  "required": [
    "ccoords",
    "lattice",
    "atomic_numbers",
    "pbc",
    "equivalent"
  ],
  "additionalProperties": false,
  "properties": {
    "ccoords": {
      "type": "array",
      "items": {
        "type": "array",
        "minItems": 3,
        "maxItems": 3,
        "items": {
          "type": "number"
        }
      }
    },
    "lattice": {
      "type": "array",
      "minItems": 3,
      "maxItems": 3,
      "items": {
        "type": "array",
        "minItems": 3,
        "maxItems": 3,
        "items": {
          "type": "number"
        }
      }
    },
    "atomic_numbers": {
      "type": "array",
      "items": {
        "type": "integer",
        "minimum": 1,
        "maximum": 99
      }
    },
    "pbc": {
      "type": "array",
      "minItems": 3,
      "maxItems": 3,
      "items": {
        "type": "boolean"
      }
    },
    "equivalent": {
      "type": "array",
      "items": {
        "type": "integer",
        "minimum": 0,
        "maximum": 999
      }
    },
    "kinds": {
      "type": "array"
    },
    "symm_equivalent": {
      "type": "array"
    }
  }
}