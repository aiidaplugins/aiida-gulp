{
    "name": "aiida-gulp",
    "author": "Chris Sewell",
    "author_email": "chrisj_sewell@hotmail.com",
    "description": "AiiDA plugin for use with GULP",
    "url": "https://github.com/chrisjsewell/aiida-gulp",
    "license": "MIT",
    "classifiers": [
        "Programming Language :: Python"
    ],
    "version": "0.1.0a0",
    "entry_points": {
        "aiida.calculations": [
            "gulp.optimize = aiida_gulp.calculations.optimize:OptCalculation",
            "gulp.single = aiida_gulp.calculations.single:SingleCalculation"
        ],
        "aiida.parsers": [
            "gulp.optimize = aiida_gulp.parsers.optimize:OptParser",
            "gulp.single = aiida_gulp.parsers.single:SingleParser"
        ]
    },
    "scripts": [
      "aiida_gulp/tests/mock_gulp"
    ],
    "setup_requires": ["reentry"],
    "include_package_data": true,
    "reentry_register": true,
    "install_requires": [
        "aiida-core>=0.12.1",
        "six",
        "jsonextended",
        "jsonschema",
        "spglib",
        "ase"
    ],
    "extras_require": {
        "testing": [
            "mock==2.0.0",
            "pgtest==1.1.0",
            "sqlalchemy-diff==0.1.3",
            "pytest==3.6.3",
            "wheel>=0.31",
            "coverage",
            "pytest-cov",
            "pytest-timeout"
        ],
        "pre-commit": [
            "pre-commit==1.11.0",
            "yapf==0.24.0",
            "prospector==1.1.2",
            "pylint==1.9.3"
        ],
        "docs": [
            "sphinx"
        ]
    }
}
