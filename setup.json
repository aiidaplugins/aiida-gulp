{
    "name": "aiida-gulp",
    "author": "Chris Sewell",
    "author_email": "chrisj_sewell@hotmail.com",
    "description": "AiiDA plugin for use with GULP",
    "url": "https://github.com/chrisjsewell/aiida-gulp",
    "license": "MIT",
    "classifiers": [
        "Programming Language :: Python"
    ],
    "version": "0.1.0a0",
    "entry_points": {
        "aiida.data": [
            "gulp = aiida_gulp.data:DiffParameters"
        ],
        "aiida.calculations": [
            "gulp = aiida_gulp.calculations:DiffCalculation"
        ],
        "aiida.parsers": [
            "gulp = aiida_gulp.parsers:DiffParser"
        ],
        "aiida.cmdline.data": [
            "gulp = aiida_gulp.data.data_cli:cli"
        ],
        "console_scripts": [
            "gulp-submit=aiida_gulp.cli:submit"
        ]
    },
    "setup_requires": ["reentry"],
    "reentry_register": true,
    "install_requires": [
        "aiida-core>=0.12.2,<1.0.0",
        "six"
    ],
    "extras_require": {
        "testing": [
            "mock==2.0.0",
            "pgtest==1.1.0",
            "sqlalchemy-diff==0.1.3",
            "wheel>=0.31",
            "coverage"
        ],
        "pre-commit": [
            "pre-commit==1.11.0",
            "yapf==0.24.0",
            "prospector==0.12.11",
            "pylint==1.9.3"
        ],
        "docs": [
            "sphinx"
        ]
    }
}
